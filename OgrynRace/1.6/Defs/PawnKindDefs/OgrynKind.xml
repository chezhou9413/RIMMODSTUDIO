<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <PawnKindDef>
        <defName>OgrynTest</defName>
        <label>欧戈苓难民</label>
        <race>Ogryn</race>
        <combatPower>100</combatPower>
        <defaultFactionType>PlayerColony</defaultFactionType>
        <backstoryFiltersOverride>
            <li>
                <categories>
                    <li>Ogryn_Backstory_Child</li>
                    <li>Ogryn_Backstory_Adult</li>
                </categories>
            </li>
        </backstoryFiltersOverride>
        <modExtensions>
            <li Class="ChezhouLib.Extension.kindRule">
                <ApparelStuffRules>
                    <li>
                        <Apparel>Pascal_OgrynBaseOutfit</Apparel>
                        <ReplaceStuff>Cloth</ReplaceStuff>
                    </li>
                </ApparelStuffRules>
            </li>
        </modExtensions>
        <xenotypeSet>
            <xenotypeChances>
                <OGR_Xenotype_Ogryn>1</OGR_Xenotype_Ogryn>
            </xenotypeChances>
        </xenotypeSet>
        <apparelRequired>
            <li>Pascal_OgrynBaseOutfit</li>
        </apparelRequired>
        <apparelTags />
        <apparelMoney>0~500</apparelMoney>
        <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <initialResistanceRange>
            <min>0</min>
            <max>0</max>
        </initialResistanceRange>
        <initialWillRange>
            <min>0.5</min>
            <max>1.0</max>
        </initialWillRange>
        <minGenerationAge>20</minGenerationAge>
        <maxGenerationAge>300</maxGenerationAge>
    </PawnKindDef>
    <PawnKindDef>
        <defName>OgrynAdvancedWarrior</defName>
        <label>欧戈苓高级近卫</label>
        <race>Ogryn</race>
        <combatPower>400</combatPower>
        <defaultFactionType>OgrynFaction</defaultFactionType>
        <backstoryFiltersOverride>
