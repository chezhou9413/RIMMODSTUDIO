    <!-- 欧戈苓商人：用于派系商队/访客队伍中的交易员身份 -->
    <PawnKindDef>
        <defName>OgrynTrader</defName>
        <label>欧戈苓商人</label>
        <race>Ogryn</race>
        <trader>true</trader>
        <combatPower>120</combatPower>
        <defaultFactionType>OgrynFaction</defaultFactionType>
        <!-- 只使用基础服装，避免过重防具影响负重与交易体验 -->
        <apparelRequired>
            <li>Pascal_OgrynBaseOutfit</li>
        </apparelRequired>
        <apparelTags />
        <apparelMoney>0~300</apparelMoney>
        <!-- 商人不携带武器，避免需要 weaponTags 配置校验 -->
        <weaponMoney>0~0</weaponMoney>
        <xenotypeSet>
            <xenotypeChances>
                <OGR_Xenotype_Ogryn>1</OGR_Xenotype_Ogryn>
            </xenotypeChances>
        </xenotypeSet>
        <!-- 年龄略放宽，保持成年与耐力形象 -->
        <minGenerationAge>22</minGenerationAge>
        <maxGenerationAge>200</maxGenerationAge>
        <!-- 人形 PawnKind 需要定义意志与反抗区间，以满足原版校验 -->
        <initialResistanceRange>
            <min>0</min>
            <max>0</max>
        </initialResistanceRange>
        <initialWillRange>
            <min>0.5</min>
            <max>1.0</max>
        </initialWillRange>
    </PawnKindDef>
</Defs> 