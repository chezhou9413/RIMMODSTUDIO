                <categories>
                    <li>Ogryn_Backstory_Child</li>
                    <li>Ogryn_Backstory_Adult</li>
                </categories>
            </li>
        </backstoryFiltersOverride>
        <canArriveManhunter>true</canArriveManhunter>
        <isFighter>true</isFighter>
        <aiAvoidCover>false</aiAvoidCover>
        <xenotypeSet>
            <xenotypeChances>
                <OGR_Xenotype_Ogryn>1</OGR_Xenotype_Ogryn>
            </xenotypeChances>
        </xenotypeSet>
        <apparelRequired>
            <li>Pascal_OgrynBaseOutfit</li>
            <li>Apparel_OgrynSentinelArmor</li>
            <li>Apparel_OgrynSentinelHelmet</li>
            <li>Apparel_OgrynGreatShield</li>
        </apparelRequired>
        <apparelTags />
        <apparelMoney>0~0</apparelMoney>
        <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
        <weaponMoney>5000~10000</weaponMoney>
        <weaponTags>
            <li>OgrynMeleeMasterCategory</li>
        </weaponTags>
        <initialResistanceRange>
            <min>30</min>
            <max>50</max>
        </initialResistanceRange>
        <initialWillRange>
            <min>20</min>
            <max>50</max>
        </initialWillRange>
        <minGenerationAge>20</minGenerationAge>
        <maxGenerationAge>200</maxGenerationAge>
    </PawnKindDef>
