  <!-- ========== 身体结构定义 ========== -->

  <BodyDef>
    <defName>LABOl_MechanicalBody</defName>
    <label>机械仿生体</label>
    <corePart>
      <def>LABOl_Torso</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>LABOl_MechanicalTorso</li>
        <li>Torso</li>
      </groups>
      <parts>

        <!-- 动力核心 -->
        <li>
          <def>LABOl_PowerCore</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
          <groups>
            <li>LABOl_MechanicalTorso</li>
            <li>Torso</li>
          </groups>
        </li>

        <!-- 冷却系统 -->
        <li>
          <def>LABOl_CoolingSystem</def>
          <coverage>0.05</coverage>
          <depth>Inside</depth>
          <groups>
            <li>LABOl_MechanicalTorso</li>
            <li>Torso</li>
          </groups>
        </li>

        <!-- 机械颈部 + 核心处理框架 -->
        <li>
          <def>LABOl_Neck</def>
          <coverage>0.075</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>LABOl_CoreProcessingFrame</def>
              <coverage>0.9</coverage>
              <parts>
                <li>
                  <def>Head</def>
                  <coverage>0.00001</coverage>
                  <depth>Outside</depth>
                  <height>Top</height>
                </li>
                <li>
                  <def>LABOl_Head</def>
                  <coverage>0.8</coverage>
                  <groups>
                    <li>LABOl_MechanicalHead</li>
                    <li>FullHead</li>
                    <li>HeadAttackTool</li>
                  </groups>
                  <parts>
                    <!-- 人格芯片 -->
                    <li>
                      <def>LABOl_PersonalityCore</def>
                      <coverage>0.3</coverage>
                      <depth>Inside</depth>
                      <groups>
                        <li>LABOl_MechanicalHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <!-- 左右光学感应器 -->
                    <li>
                      <def>LABOl_Optic</def>
                      <customLabel>左光学感应器</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LABOl_MechanicalHead</li>
                        <li>FullHead</li>
                        <li>Eyes</li>
                      </groups>
                      <woundAnchorTag>LeftEye</woundAnchorTag>
                      <flipGraphic>true</flipGraphic>
                    </li>
                    <li>
                      <def>LABOl_Optic</def>
                      <customLabel>右光学感应器</customLabel>
                      <coverage>0.15</coverage>
                      <groups>
                        <li>LABOl_MechanicalHead</li>
                        <li>FullHead</li>
                        <li>Eyes</li>
                      </groups>
                      <woundAnchorTag>RightEye</woundAnchorTag>
                    </li>
                    <!-- 左右听觉接收器 -->
                    <li>
                      <def>LABOl_Audio</def>
                      <customLabel>左听觉接收器</customLabel>
                      <coverage>0.1</coverage>
                      <groups>
                        <li>LABOl_MechanicalHead</li>
                        <li>FullHead</li>
                      </groups>
                      <flipGraphic>true</flipGraphic>
                    </li>
                    <li>
                      <def>LABOl_Audio</def>
                      <customLabel>右听觉接收器</customLabel>
                      <coverage>0.1</coverage>
                      <groups>
                        <li>LABOl_MechanicalHead</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>

        <!-- 左机械肩部 + 左机械臂 + 左机械手 -->
        <li>
          <def>LABOl_Shoulder</def>
          <customLabel>左机械肩部</customLabel>
          <coverage>0.12</coverage>
          <woundAnchorTag>LeftShoulder</woundAnchorTag>
          <flipGraphic>true</flipGraphic>
          <groups>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>LABOl_Arm</def>
              <customLabel>左机械臂</customLabel>
              <coverage>0.77</coverage>
              <flipGraphic>true</flipGraphic>
              <groups>
                <li>LABOl_MechanicalArms</li>
                <li>Arms</li>
              </groups>
              <parts>
                <li>
                  <def>LABOl_Hand</def>
                  <customLabel>左机械手</customLabel>
                  <coverage>0.14</coverage>
                  <height>Bottom</height>
                  <flipGraphic>true</flipGraphic>
                  <groups>
                    <li>LABOl_MechanicalHands</li>
                    <li>Hands</li>
                  </groups>
                  <parts>
                    <!-- 左手机械手指 -->
                    <li>
                      <def>LABOl_Finger</def>
                      <customLabel>左小指</customLabel>
                      <coverage>0.06</coverage>
                      <groups>
                        <li>LABOl_MechanicalHands</li>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>LABOl_Finger</def>
                      <customLabel>左无名指</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>LABOl_MechanicalHands</li>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>LABOl_Finger</def>
                      <customLabel>左中指</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>LABOl_MechanicalHands</li>
                        <li>Hands</li>
                        <li>LeftHand</li>
                        <li>MiddleFingers</li>
                      </groups>
                    </li>
                    <li>
                      <def>LABOl_Finger</def>
                      <customLabel>左食指</customLabel>
                      <coverage>0.07</coverage>
                      <groups>
                        <li>LABOl_MechanicalHands</li>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                    <li>
                      <def>LABOl_Finger</def>
                      <customLabel>左拇指</customLabel>
                      <coverage>0.08</coverage>
                      <groups>
                        <li>LABOl_MechanicalHands</li>
                        <li>Hands</li>
                        <li>LeftHand</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>

        <!-- 右机械肩部 + 右机械臂 + 右机械手 -->
        <li>
          <def>LABOl_Shoulder</def>
          <customLabel>右机械肩部</customLabel>
          <coverage>0.12</coverage>
          <woundAnchorTag>RightShoulder</woundAnchorTag>
          <groups>
